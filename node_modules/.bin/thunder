#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_thunder-vip@1.0.10@thunder-vip/bin/thunder" "$@"
  ret=$?
else 
  node  "$basedir/../_thunder-vip@1.0.10@thunder-vip/bin/thunder" "$@"
  ret=$?
fi
exit $ret
